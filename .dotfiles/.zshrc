# rbenv
export PATH="$HOME/.rbenv/bin:$PATH"
eval "$(rbenv init -)"
HISTSIZE=100000
SAVEHIST=100000
EDITOR=vim
VISUAL=vim
bindkey -e
# Base16 Shell                                                 
BASE16_SHELL="$HOME/.config/base16-shell/base16-default.dark.sh"
[[ -s $BASE16_SHELL ]] && source $BASE16_SHELL
